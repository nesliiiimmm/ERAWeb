
@{
    ViewBag.Title = "Resultlar";
}

@model List<AYAK.Common.NetCore.Result>



<div class="row">
    <div class="col-xl-4">

    </div>
    <div class="col-lg-12 col-xxl-12">
        <div class="card card-custom card-stretch gutter-b">
            <div class="card-header border-0 ">
                <h3 class="card-title align-items-start flex-column">
                    <span class="card-label font-weight-bolder text-dark">Result Listesi</span>
                </h3>

            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-head-custom table-vertical-center" id="kt_advance_table_widget_1">
                        <thead>
                            <tr class="text-left">

                                <th class="pr-0">Date</th>
                                <th class="pr-0">Description</th>
                                <th class="pr-0">Message</th>
                                <th class="pr-0">State</th>
                                <th class="pr-0">Data</th>
                                <th class="pr-0">DataReady</th>
                                <th class="pr-0">ExDetail</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="pl-0">@item.Date.ToString("yy/MM/dd HH:mm:ss")</td>
                                    <td class="pl-0">@item.Description</td>
                                    <td class="pl-0">@item.Message</td>
                                    <td class="pl-0">@item.State</td>
                                    <td class="pl-0">@item.Data</td>
                                    <td class="pl-0">@item.DataReady</td>
                                    <td class="pl-0">@item.ExDetail</td>

                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
